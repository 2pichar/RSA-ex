{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "pwa-node",
      "name": "Run Current File",
      "request": "launch",
      "program": "${file}",
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start:ts",
      "request": "launch",
      "command": "npm run start:ts",
      "cwd": "${workspaceFolder}"
    }
  ]
}